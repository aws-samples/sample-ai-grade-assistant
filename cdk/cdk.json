{
  "app": "npx ts-node --prefer-ts-exts bin/app.ts",
  "watch": {
    "include": [
      "**",
      "../lambda"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "**/*.d.ts",
      "**/*.js",
      "tsconfig.json",
      "package*.json",
      "yarn.lock",
      "node_modules",
      "test",
      ".venv",
      "../lambda/.venv",
      "../lambda/.student_advisor_venv"
    ]
  },
  "context": {
    "@aws-cdk/customresources:installLatestAwsSdkDefault": false
  }
}
